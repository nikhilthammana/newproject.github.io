//All factors of N 
//prime number testing
/*
#include<stdio.h>
int main()
{
    int N;
    scanf("%d",&N);
    int i=1;
    int count=0;
    printf("factors of %d:",N);
    while(i<=N)
    {
       if(N%i==0)
       {
           printf("%d",i);
           count=count+1;
       }
        i=i+1;
    }
    printf("\n");
    if(count==2)
    {
        printf("prime number");
    }
    else{
        printf("not prime number");
    }
}
output:
6
factors of 6:1236
not prime number
*/
