//pyramid pattern
/*
#include<stdio.h>
int main()
{
    int N;
    scanf("%d",&N);
    int i=1;
    while(i<=N)
    {
        int j=1;
        while(j<=N-i)
        {
            printf(" ");
            j=j+1;
        }
        int k=1;
        while(k<=i)
        {
            printf("* ");
            k=k+1;
        }
        printf("\n");
        i=i+1;
    }
}
output:
5
    * 
   * * 
  * * * 
 * * * * 
* * * * * 
*/
//prime number
/*
#include<stdio.h>
int main()
{
    int N;
    scanf("%d",&N);
    int flag=0;
    int i=2;
    while(i<N)
    {
        if(N%i==0)
        {
            flag=1;
            break;
        }
    }
    if(flag==0)
    {
        printf("prime number");
    }
    else{
        printf("Not prime number");
    }
}
*/

//strong number
/*
#include<stdio.h>
int main()
{
    int V;
    scanf("%d",&V);
    int N=V;
    int sum=0;
    while(N>0)
    {
        int A=N%10;
        int fact=1;
        int j=1;
        while(j<=A)
        {
            fact=fact*j;
            j=j+1;
        }
        sum=sum+fact;
        N=N/10;
    }
    if(sum==V)
    {
        printf("strong");
    }
    else{
        printf("Not");
    }
}
output:
145
strong
*/

//strong number between range
/*
#include<stdio.h>
int main()
{
    int start;
    int end;
    scanf("%d %d",&start,&end);
    while(start<=end)
    {
        int dup=start;
        int sum=0;
        while(dup>0)
        {
          int A=dup%10;
          int fact=1;
          int j=1;
        while(j<=A)
        {
            fact=fact*j;
            j=j+1;
        }
        sum=sum+fact;
        dup=dup/10;
        }
        if(start==sum)
        {
        printf("strong: %d\n",start);
        }  
        start=start+1;
    }
}
output:
1 1000
strong: 1
strong: 2
strong: 145
*/

//GCD OR HCF
//HIGHEST COMMON FACTOR
// 2 6
/*
#include<stdio.h>
int main()
{
    int a=2,b=6;
    int d;
    if(a<=b)
    {
       d=a; 
    }
    else{
        d=b;
    }
    int x=0,i=1;
    while(i<=d)
    {
        if(a%i==0 && b%i==0)
        {
            x=i;
        }
       i=i+1; 
    }
    printf("%d",x);
}
output:
2
*/
//lcm-least common multiple
//5
//7
//high>=lcm<=5*7
//7>=lcm<=35
/*
#include<stdio.h>
int main()
{
    int a,b;
    scanf("%d %d",&a,&b);
    int flag=1;
    int prod=a*b;
    int high;
    if (a>=b)
    {
        high=a;
    }
    else{
        high=b;
    }
    int i=high;
    while(i<=prod)
    {
    while(i%a==0 && i%b==0 && flag==1)
    {
        printf("%d",i);
        flag=flag+1;
    }
        i=i+1;
    }
}
output:
8 9
72
*/

