//logical operators
/*
#include<stdio.h>
int main()
{
    int a=5 ,b=6;
    if(a>b)
    {
        printf("True");
    }
    else{
        printf("False");
    }
    return 0;
}
output:
False
*/
/*
#include<stdio.h>
int main()
{
    int a=8 ,b=6,c=7;
    int d=a>b && c>b;
    printf("%d",d);
    
    return 0;
}
output:
1
*/
/*
#include<stdio.h>
int main()
{
    int a=8 ,b=6,c=7;
    int d=a>b || c>b;
    printf("%d",d);
    
    return 0;
}
output:
1
 */
//modulo operator
/*
#include<stdio.h>
int main()
{
    int x,y;
    scanf("%d %d",&x,&y);
    //dividend=divisor*quotinet+remainder
    //remainder=dividend-divisor*quotient
    int q=x/y;
    int r=x-y*q;
    printf("%d\n",r);
    return 0;
}
output:
5 6
5
*/
/*
#include<stdio.h>
int main()
{
    int x,y;
    scanf("%d %d",&x,&y);
    printf("%d",x%y);
    return 0;
}
output:
5 6
5
*/

