//sum of terms:::: 1+x+x^2/2!+x^3/3!......
/*
#include<stdio.h>
int main()
{
    int N,x;
    scanf("%d %d",&N,&x);
    double sum=1;
    for(int i=1;i<=N;i++)
    {
        int fact=1;
        int pow=1;
        for(int j=1;j<=i;j++)
        {
            fact=fact*j;
            pow=pow*x;
        }
        sum=sum+pow*1.0/fact;
    }
    printf("%lf",sum);
}

output:
2 1
2.500000
*/

