INTEGER:
 INT-%d,LONG INT-%ld,LONG LONG-%lld;

FLOAT:
 FLOAT-%f,DOUBLE-%lf,LONG DOUBLE-%llf;
 
CHAR;
*/

//type casting
//int to float
/*
#include<stdio.h>
int main()
{
    int a=5;
    printf("%f \n",(float)a);//float
    int b=5.7685;
    printf("%f \n",a);//float
    int c=5.34627;
    printf("%d \n",a);//integer
    return 0;
}
//output

5.000000 
5.000000 
5 
*/
//float to int
/*
#include<stdio.h>
