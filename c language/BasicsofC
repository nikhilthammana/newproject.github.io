//casing matters
/*
#include<stdio.h>
int main()
{
    int a=5,A=6;
    printf("%d %d",a,A);
    return 0;
}
output
5 6
*/

//to know the size of data type
/*
#include<stdio.h>
#include<limits.h>
int main()
{
    int a,b;
    a=INT_MIN;
    b=INT_MAX;
    printf("%d %d",a,b);
    return 0;
}
output
-2147483648 2147483647
*/

//char size
/*
#include<stdio.h>
#include<limits.h>
